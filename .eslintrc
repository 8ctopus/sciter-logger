{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended", "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "html"
    ],
    "rules": {
        "indent": [
            "error",
            4,
            { "SwitchCase": 1 }
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": [
            "warn"
        ],
        "max-len": [
            "warn",
            120
        ],
        "brace-style": [
            "off"
        ],
        "curly": [
            "off"
        ],
        "new-cap": [
            "error"
        ],
        "require-jsdoc": [
            "off"
        ],
        "valid-jsdoc": [
            "off"
        ]
    }
}
